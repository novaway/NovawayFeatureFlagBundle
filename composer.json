{
    "name": "novaway/feature-flag-bundle",
    "description": "Very KISS bundle to manage features flag",
    "type": "symfony-bundle",
    "keywords": ["symfony", "feature", "flag"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy DECOOL",
            "email": "jeremy@novaway.fr"
        }
    ],
    "require": {
        "php": ">=5.6",
        "doctrine/annotations": "^1.3",
        "symfony/framework-bundle": "~2.7|~3.0|~4.0",
        "symfony/yaml": "~2.7|~3.0|~4.0"
    },
    "require-dev": {
        "atoum/atoum": "^2.8|^3.0",
        "atoum/stubs": "^2.5",
        "symfony/asset": "~3.0|~4.0",
        "symfony/browser-kit": "~2.7|~3.0|~4.0",
        "symfony/css-selector": "~2.7|~3.0|~4.0",
        "symfony/templating": "~2.7|~3.0|~4.0",
        "symfony/twig-bundle": "~2.7|~3.0|~4.0",
        "twig/twig": "^1.12|^2.0"
    },
    "suggest": {
        "twig/twig": "To check feature in your Twig templates"
    },
    "autoload": {
        "psr-4": {"Novaway\\Bundle\\FeatureFlagBundle\\": ""},
        "exclude-from-classmap": ["Tests/"]
    }
}
