{
    "name": "novaway/feature-flag-bundle",
    "description": "Very KISS bundle to manage features flag",
    "type": "symfony-bundle",
    "keywords": ["symfony", "feature", "flag"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy DECOOL",
            "email": "jeremy@novaway.fr"
        }
    ],
    "require": {
        "php": ">= 7.3",
        "doctrine/annotations": "^1.10.4",
        "symfony/framework-bundle": "~4.4|~5.0",
        "symfony/yaml": "~4.4|~5.0"
    },
    "require-dev": {
        "atoum/atoum": "^4.0",
        "atoum/stubs": "^2.5",
        "symfony/asset": "~3.0|~4.0|~5.0",
        "symfony/browser-kit": "~4.0|~5.0",
        "symfony/css-selector": "~4.0|~5.0",
        "symfony/templating": "~4.0|~5.0",
        "symfony/twig-bundle": "~4.0|~5.0",
        "twig/twig": "^v1.42.3|^2.0"
    },
    "suggest": {
        "twig/twig": "To check feature in your Twig templates"
    },
    "autoload": {
        "psr-4": {"Novaway\\Bundle\\FeatureFlagBundle\\": ""},
        "exclude-from-classmap": ["Tests/"]
    }
}
